	pmcid = {PMC5803269},
	keywords = {Humans, Gene Expression Profiling, Sequence Analysis, RNA, Aging, Transcriptome, Alzheimer Disease, Middle Aged, Aged, Microglia, Autopsy, Prefrontal Cortex, Prospective Studies, Atlases as Topic, Cohort Studies, Gene Library},
