	keywords = {acute disseminated encephalomyelitis, myelin oligodendrocyte glycoprotein antibodies, optic neuritis, outcomes, therapy},
	pages = {127--137},
	file = {Full Text:C\:\\Users\\epat8919\\Zotero\\storage\\4AGMCE9B\\Ramanathan et al. - 2018 - Clinical course, therapeutic responses and outcome.pdf:application/pdf}
